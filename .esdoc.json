{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.js$", "\\.jsx$"],
  "excludes": ["registerServiceWorker.js"],
  "plugins": [
    { "name": "esdoc-flow-type-plugin", "option": { "enable": true } },
    {"name": "esdoc-standard-plugin"},
    {"name": "esdoc-jsx-plugin", "option": {"enable": true}}
  ]
}
